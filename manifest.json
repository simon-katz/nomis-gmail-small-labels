{
    "manifest_version": 3,

    "name": "Nomis Gmail Small Labels",
    "version": "0.1.0",
    "description": "Make Gmail labels small in the message list pane.",

    "host_permissions": [
        "https://mail.google.com/"
    ],

    "content_scripts": [{
        "css": ["styles.css"],
        "matches": ["https://mail.google.com/*"]
    }]

}
